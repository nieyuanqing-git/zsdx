@using com.Bynonco.JqueryEasyUI.Core;
@{
    var animalPrivilige = com.Bynonco.LIMS.BLL.Business.Privilige.PriviligeFactory.GetAnimalPrivilige(User.Identity.Name);
    var dataGrid = Html.JDataGrid("tbMyAnimalList")
            .SetUrl(Url.Action("GetMyGridViewAnimalList", "Animal", new { time = DateTime.Now.Millisecond.ToString() }))
            .SetHeight(500)
            .SetPageSize(15)
            .SetSortName("Name");
            dataGrid.SetShowDeleteButton(true)
            .SetShowPrintButton(true);
            dataGrid.AddFrozenColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("编码", "Id").SetHidden(true))
            .AddFrozenColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("名称", "NameStr").SetWidth(223).SetAlign(Align.left))
            .AddFrozenColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("库存状态", "StoreStatusStr").SetWidth(60).SetAlign(Align.center))
            .AddFrozenColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("状态", "StatusStr").SetWidth(40).SetAlign(Align.center))
            .AddFrozenColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("所属品系", "AnimalCategoryName").SetWidth(120).SetAlign(Align.center))
            .AddFrozenColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("性别", "SexStr").SetWidth(60).SetAlign(Align.center))
            .AddFrozenColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("购置人", "OwnerName").SetWidth(60).SetAlign(Align.center))
            .AddFrozenColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("存放位置", "AnimalPos").SetSortable(false).SetWidth(200).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("是否停用", "IsStopStr").SetWidth(60).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("批次编号", "Code").SetWidth(130).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("计费方式", "ChargeTypeStr").SetWidth(100).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("价格(元)", "UnitPriceStr").SetWidth(100).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("体重(kg)", "WeightStr").SetWidth(100).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("出生天数", "Age").SetWidth(100).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("所属级别", "GradeName").SetWidth(100).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("入库人", "InOperatorName").SetWidth(100).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("入库时间", "InDateStr").SetWidth(120).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("登记死亡操作人", "RegisterDeathOperatorName").SetWidth(100).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("死亡时间", "DieTimeStr").SetWidth(120).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("确认死亡时间", "ConfirmDeathDateStr").SetWidth(120).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("确认死亡状态", "ConfirmDeathStatusStr").SetWidth(100).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("取材申请时间", "ApplyGettingOutDateStr").SetWidth(120).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("取材申请人", "ApplyGettingOutOperatorName").SetWidth(80).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("取材审核人", "GettingOutAuditOperatorName").SetWidth(80).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("取材审核状态", "GettingOutAuditStatusStr").SetWidth(80).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("最后扣费时间", "LatestCostDeductTimeStr").SetWidth(120).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("联系人", "LinkMan").SetWidth(120).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("联系电话", "LinkTelNo").SetWidth(120).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("助记符", "InputStr").SetWidth(150).SetAlign(Align.center))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("描述", "Description").SetWidth(500).SetAlign(Align.left))
            .AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("备注", "Remark").SetWidth(500).SetAlign(Align.left))
            .SetShowSearchButton(true)
            .SetSearchScript("doShowSearch()");
  }
  @(dataGrid.ToMvcString())    