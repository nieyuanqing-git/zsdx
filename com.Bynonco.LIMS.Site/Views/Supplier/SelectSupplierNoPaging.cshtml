@{
    var limitSupplierIdList = ViewData["LimitSupplierIdList"];
}
<div id="divSelectSupplierDialog">
     <div class="z-panel-grid hide">
        <div class="search-inner-box">
            @Html.Partial("../Supplier/Search")
        </div>
    </div>
    <div>@Html.Partial("../Supplier/SelectSupplierNoPagingList")</div>
</div>
<script type="text/javascript">
    function doSupplierSearch() {
        var queryExpression = generateSupplierQueryExpress();
        var rows = $('#tbSelectSupplierNoPagingList').datagrid("getRows");
        if (rows && rows.length > 0) {
            for (var i = rows.length -1; i >=0 ; i--) {
                $('#tbSelectSupplierNoPagingList').datagrid("deleteRow",i);
            }
        }
        $("#tbSelectSupplierNoPagingList").datagrid('loadData', { total: 0, rows: [] });
        $("#tbSelectSupplierNoPagingList").datagrid("load", { "queryExpression": queryExpression.trim(), "time": new Date().getMilliseconds() });
        return false;
    }
</script>