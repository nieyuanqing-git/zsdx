@model com.Bynonco.LIMS.Model.Business.StatisticsInfo
@if (Model != null && (Model.IsShowUnAuditSamplaApplyCount))
{
    <p><span class="icon icon-yang">&nbsp;</span>送样</p>
    <table class="edit-info admin-statistics">
        <tr>
            <td class='col-first'><span class="icon icon-globe" title='管理相关' style='margin-right: 0;'>&nbsp;</span></td>
            @if (Model.IsShowUnAuditSamplaApplyCount)
            {
                <th><a href="#" onclick="doLoadUnAuditSampleApply();return false;">未审核的送样记录</a></th>
                <td class="col-last">
                    <a href="#" onclick="doLoadUnAuditSampleApply();return false;">@Model.UnAuditSampleApplyCount</a>
                </td>
            }
        </tr>
    </table>
}