@model com.Bynonco.LIMS.Model.Business.StatisticsInfo
@if (Model != null && Model.IsShowAuditUser)
{
    <p><span class="icon icon-user">&nbsp;</span>用户</p>
    <table class="edit-info admin-statistics">
        <tr>
            <td class='col-first'><span class="icon icon-globe" title='管理相关' style='margin-right: 0;'>&nbsp;</span></td>
            @if (Model.IsShowAuditWaittingUser)
            {
                <th><a href="#" onclick="doLoadAuditWaittingUser();return false;">等待审核用户</a></th>
                <td class='@(Model.IsShowAuditRejectUser ? "" : "col-last")'>
                    <a href="#" onclick="doLoadAuditWaittingUser();return false;">@Model.AuditWaittingUser</a>
                </td>
            }
            @if (Model.IsShowAuditRejectUser)
            {
                <th><a href="#" onclick="doLoadAuditRejectUser();return false;">审核不通过用户</a></th>
                <td class="col-last">
                    <a href="#" onclick="doLoadAuditRejectUser();return false;">@Model.AuditRejectUser</a>
                </td>
            }
        </tr>
    </table>
}
