@using com.Bynonco.JqueryEasyUI.Core;
@using com.Bynonco.LIMS.Model.Enum;
@{
    Layout = ViewBag.Layout;
}
<div class="search-grid hide">
    <div class="search-inner-box">
        @Html.Partial("../MaterialPurchase/Search")
    </div>
</div>
<div class="layout-center-full">
    <ul class="tabs" style="border-bottom:0;padding-top:10px;">
        <li class="tabs-selected" id=''><a class="tabs-inner" href="javascript:void(0)"><span class="tabs-title">全部</span><span class="tabs-icon"></span></a></li>
        <li status='@((int)MaterialPurchaseStatus.OperatorAuditPass)'><a class="tabs-inner" href="javascript:void(0)"><span class="tabs-title">等待验收入库</span><span class="tabs-icon"></span></a></li>
        <li status='@((int)MaterialPurchaseStatus.InputPass)'><a class="tabs-inner" href="javascript:void(0)"><span class="tabs-title">验收入库通过</span><span class="tabs-icon"></span></a></li>
        <li status='@((int)MaterialPurchaseStatus.InputReject)'><a class="tabs-inner" href="javascript:void(0)"><span class="tabs-title">验收入库不通过</span><span class="tabs-icon"></span></a></li>
    </ul>
    @Html.Partial("../MaterialPurchase/List")
</div>
@Html.Partial("../MaterialPurchase/ScriptContainer")
@Html.Partial("../MaterialInfo/SelectMaterialInfoNoPagingContainer")
@Html.Partial("../MaterialInfo/SelectMaterialSupplierNoPagingContainer")
@Html.Partial("../MaterialInput/ScriptContainer")
@Html.Partial("../MaterialUserAccount/ScriptContainer")