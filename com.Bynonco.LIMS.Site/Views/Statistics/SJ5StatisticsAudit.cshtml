@using com.Bynonco.JqueryEasyUI.Core;
@{
    var statisticsPrivilige = com.Bynonco.LIMS.BLL.Business.Privilige.PriviligeFactory.GetStatisticsPrivilige(User.Identity.Name);
}
<ul class="search">
    @if (statisticsPrivilige.IsEnableSJ5StatisticsAuditPass)
    {
        <li style='float:none;'>
            @Html.JLinkButton("btnSJ5StatisticsAuditPass", "审核通过").SetIconCls("icon-pass").SetOnclick("doSJ5StatisticsAuditPass('');").ToMvcString()
        </li>
    }
    @if (statisticsPrivilige.IsEnableSJ5StatisticsAuditReject)
    {
        <li style='float:none;'>
            @Html.JLinkButton("btnSJ5StatisticsAuditReject", "审核不通过").SetIconCls("icon-no").SetOnclick("doSJ5StatisticsAuditReject('');").ToMvcString()
        </li>
    }
    @if (statisticsPrivilige.IsEnableSJ5StatisticsAuditWaitting)
    {
        <li style='float:none;'>
            @Html.JLinkButton("btnSJ5StatisticsAuditWaitting", "置为待审").SetIconCls("icon-info").SetOnclick("doSJ5StatisticsAuditWaitting('');").ToMvcString()
        </li>
    }
    <li style='float:none; height:25px;'>
        @Html.JLinkButton("btnSJ5StatisticsAuditClose", "关闭").SetIconCls("icon-cancel").SetOnclick("doCloseSearch();").ToMvcString()
    </li>
</ul>
