@using com.Bynonco.JqueryEasyUI.Core;
@{
    var statisticsPrivilige = com.Bynonco.LIMS.BLL.Business.Privilige.PriviligeFactory.GetStatisticsPrivilige(User.Identity.Name);
}
<ul class="search">
    @if (statisticsPrivilige.IsEnableSJ4StatisticsAuditPass)
    {
        <li style='float:none;'>
            @Html.JLinkButton("btnSJ4StatisticsAuditPass", "审核通过").SetIconCls("icon-pass").SetOnclick("doSJ4StatisticsAuditPass('');").ToMvcString()
        </li>
    }
    @if (statisticsPrivilige.IsEnableSJ4StatisticsAuditReject)
    {
        <li style='float:none;'>
            @Html.JLinkButton("btnSJ4StatisticsAuditReject", "审核不通过").SetIconCls("icon-no").SetOnclick("doSJ4StatisticsAuditReject('');").ToMvcString()
        </li>
    }
    @if (statisticsPrivilige.IsEnableSJ4StatisticsAuditWaitting)
    {
        <li style='float:none;'>
            @Html.JLinkButton("btnSJ4StatisticsAuditWaitting", "置为待审").SetIconCls("icon-info").SetOnclick("doSJ4StatisticsAuditWaitting('');").ToMvcString()
        </li>
    }
    <li style='float:none; height:25px;'>
        @Html.JLinkButton("btnSJ4StatisticsAuditClose", "关闭").SetIconCls("icon-cancel").SetOnclick("doCloseSearch();").ToMvcString()
    </li>
</ul>
