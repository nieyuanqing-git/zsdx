@using com.Bynonco.JqueryEasyUI.Core;
@{
    var statisticsPrivilige = com.Bynonco.LIMS.BLL.Business.Privilige.PriviligeFactory.GetStatisticsPrivilige(User.Identity.Name);
}
<ul class="search">
    @if (statisticsPrivilige.IsEnableSJ2StatisticsAuditPass)
    {
        <li style='float:none;'>
            @Html.JLinkButton("btnSJ2StatisticsAuditPass", "审核通过").SetIconCls("icon-pass").SetOnclick("doSJ2StatisticsAuditPass('');").ToMvcString()
        </li>
    }
    @if (statisticsPrivilige.IsEnableSJ2StatisticsAuditReject)
    {
        <li style='float:none;'>
            @Html.JLinkButton("btnSJ2StatisticsAuditReject", "审核不通过").SetIconCls("icon-no").SetOnclick("doSJ2StatisticsAuditReject('');").ToMvcString()
        </li>
    }
    @if (statisticsPrivilige.IsEnableSJ2StatisticsAuditWaitting)
    {
        <li style='float:none;'>
            @Html.JLinkButton("btnSJ2StatisticsAuditWaitting", "置为待审").SetIconCls("icon-info").SetOnclick("doSJ2StatisticsAuditWaitting('');").ToMvcString()
        </li>
    }
    <li style='float:none; height:25px;'>
        @Html.JLinkButton("btnSJ2StatisticsAuditClose", "关闭").SetIconCls("icon-cancel").SetOnclick("doCloseSearch();").ToMvcString()
    </li>
</ul>
