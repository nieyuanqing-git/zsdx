@using com.Bynonco.JqueryEasyUI.Core;
@using com.Bynonco.LIMS.Model.Enum;
@{
    Layout = ViewBag.Layout;
}
<div class="search-grid hide">
    <div class="search-inner-box">
        @Html.Partial("../ShareFundApply/Search")
    </div>
</div>
<div>
    <ul class="tabs" style="border-bottom:0;padding-top:10px;">
        <li class="tabs-selected" status=''><a class="tabs-inner" href="javascript:void(0)"><span class="tabs-title">全部</span><span class="tabs-icon"></span></a></li>
        <li status='@((int)ShareFundApplyStatus.WaitingAudit)'><a class="tabs-inner" href="javascript:void(0)"><span class="tabs-title">等待审核</span><span class="tabs-icon"></span></a></li>
        <li status='@((int)ShareFundApplyStatus.AuditPass)'><a class="tabs-inner" href="javascript:void(0)"><span class="tabs-title">审核通过</span><span class="tabs-icon"></span></a></li>
        <li status='@((int)ShareFundApplyStatus.AuditReject)'><a class="tabs-inner" href="javascript:void(0)"><span class="tabs-title">审核不通过</span><span class="tabs-icon"></span></a></li>
    </ul>
    @Html.Partial("../ShareFundApply/List")
</div>
@Html.Partial("../Equipment/SelectEquipmentNoPagingContainer")
@Html.Partial("../ShareFundApply/ScriptContainer")
