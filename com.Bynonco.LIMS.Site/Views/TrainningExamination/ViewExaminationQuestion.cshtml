@model IList<com.Bynonco.LIMS.Model.View.ViewTrainningQuestion>
<div id="divTrainningExaminationQuestionView" style="width:700px;height:500px;padding:10px;">
    @if (Model != null && Model.Count() > 0)
    {
        for (int i = 0; i < Model.Count; i++)
        {
            var item = Model[i];
            ViewData["QuestionNo"] = i+1;
            @Html.Partial("../TrainningQuestion/ViewQuestionInfo", item)
        }
    }
</div> 
