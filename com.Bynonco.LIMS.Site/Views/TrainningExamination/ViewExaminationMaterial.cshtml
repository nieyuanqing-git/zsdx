@model IList<com.Bynonco.LIMS.Model.View.ViewTrainningMaterial>
<div id="divTrainningExaminationMaterialView" style="width:700px;height:500px;padding:10px;">
    @if (Model != null && Model.Count() > 0)
    {
        for (int i = 0; i < Model.Count; i++)
        {
            var item = Model[i];
            ViewData["MaterialNo"] = i+1;
            @Html.Partial("../TrainningMaterial/ViewMaterialInfo", item)
        }
    }
</div> 
