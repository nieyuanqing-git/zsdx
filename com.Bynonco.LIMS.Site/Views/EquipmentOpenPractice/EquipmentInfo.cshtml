@model com.Bynonco.LIMS.Model.EquipmentOpenPracticeEquipment
@if (Model != null && Model.Equipment != null)
{
    <tr>
        <td style='text-align:center;'>@Model.Equipment.Name</td>
        <td style='text-align:center;'>@(Model.Equipment.Organization != null ? Model.Equipment.Organization.Name : "")</td>
        <td style='text-align:center;'>@Math.Round(Model.ApplyHours,2).ToString()</td>
        <td style='text-align:center;'>@Math.Round(Model.UsedApplyHours,2).ToString()</td>
        <td style='text-align:center;'>@Math.Round(Model.ApplyMoney,2).ToString()</td>
        <td style='text-align:center;'>@Math.Round(Model.UsedApplyMoney,2).ToString()</td>
    </tr>
}