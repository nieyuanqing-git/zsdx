@model com.Bynonco.LIMS.Model.EquipmentOpenPracticeMaterial
@using com.Bynonco.JqueryEasyUI.Core; 
@if (Model != null)
{
    <tr>
        <td style='text-align:center;'>@Model.MaterialName</td>
        <td style='text-align:center;'>@Math.Round(Model.ApplyMoney,2).ToString()</td>
        <td style='text-align:center;'>@(!Model.UsedUnitPrice.HasValue ? "" : Math.Round(Model.UsedUnitPrice.Value,2).ToString())</td>
        <td style='text-align:center;'>@(!Model.UsedCount.HasValue ? "" : Model.UsedCount.Value.ToString())</td>
        <td style='text-align:center;'>@(!Model.UsedTotalMoney.HasValue ? "" : Math.Round(Model.UsedTotalMoney.Value,2).ToString())</td>
    </tr>
}