<script type="text/javascript">
$(function () {
    var $backToTopTxt = "返回顶部", $backToTopEle = $('<div class="backToTop"></div>').appendTo($("body"))
    .text($backToTopTxt).attr("title", $backToTopTxt).click(function () {
        $("html, body").animate({ scrollTop: 0 }, 120);
    }), $backToTopFun = function () {
        var st = $(document).scrollTop(), winh = $(window).height();
        (st > 0) ? $backToTopEle.show() : $backToTopEle.hide();
        //IE6下的定位

        if (!window.XMLHttpRequest) {
            $backToTopEle.css("top", st + winh - 166);
            $(".body-top").css("top", st);
        }
    };
    $(window).bind("scroll", $backToTopFun);
    $(function () { $backToTopFun(); });
});

function backToLogin(fName, equipmentId, equipmentName) {
    if (fName && equipmentId && equipmentName) {
        if ($("#TopReturnUrl").length <= 0)
            $("body").append("<input type='hidden' value='' name='TopReturnUrl' id='TopReturnUrl'>");
        $("#TopReturnUrl").attr("fName", fName);
        $("#TopReturnUrl").attr("equipmentId", equipmentId);
        $("#TopReturnUrl").attr("equipmentName", equipmentName);
    }
    else {
        $("#TopReturnUrl").remove();
    }
    setTimeout('showTopLogin();', 100);
    return false;
}
function showTopLogin() {
    $(".pop-login-box").removeClass("hide");
     $("#frmTopLogin").find("#LoginName").focus();
}
</script>