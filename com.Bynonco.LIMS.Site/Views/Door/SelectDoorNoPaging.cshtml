@{
    var limitDoorIdList = ViewData["LimitDoorIdList"];
}
<div id="divSelectDoorDialog">
     <div class="z-panel-grid hide">
        <div class="search-inner-box">
            @Html.Partial("../Door/Search")
        </div>
    </div>
    <div>@Html.Partial("../Door/SelectDoorNoPagingList")</div>
</div>
<script type="text/javascript">
    function doDoorSearch() {
        var queryExpression = generateDoorQueryExpress();
        var rows = $('#tbSelectDoorNoPagingList').datagrid("getRows");
        if (rows && rows.length > 0) {
            for (var i = rows.length -1; i >=0 ; i--) {
                $('#tbSelectDoorNoPagingList').datagrid("deleteRow",i);
            }
        }
        $("#tbSelectDoorNoPagingList").datagrid('loadData', { total: 0, rows: [] });
        $("#tbSelectDoorNoPagingList").datagrid("load", { "queryExpression": queryExpression.trim(), "time": new Date().getMilliseconds() });
        return false;
    }
</script>