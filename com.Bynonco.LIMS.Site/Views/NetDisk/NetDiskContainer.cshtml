@using com.Bynonco.LIMS.Model;
@{
    Layout = ViewBag.Layout;
    var user = ViewData["User"] == null ? null : (User)ViewData["User"];
    var src = string.Format("../../WebDisk/Main.html?userId={0}&password={1}&time={2}", user == null ? "" : user.Id.ToString(), user == null ? "" : user.LoginPassword, DateTime.Now.Millisecond);
}
<div style="width:100%;height:680px;">
    <iframe height="100%" width="100%" frameborder="0" src="@src"></iframe>
</div>
