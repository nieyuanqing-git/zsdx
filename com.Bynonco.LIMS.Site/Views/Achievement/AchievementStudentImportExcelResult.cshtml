@model IList<com.Bynonco.LIMS.Model.AchievementStudent>
<style type="text/css">
    .edit-table td{text-align:center;}
    #divAchievementStudentImportExcelResult{padding-bottom:10px;overflow-y:auto;}
</style>
@if (Model != null && Model.Count() > 0)
{
    var susuccee = Model.Count();
    var error = 0;
    var total = Model.Count();
    <p>导入结果: 成功[@(susuccee)],  失败[@(error)], 总共[@(total)]</p>
    <table border='1' cellpadding='0' cellspacing='0' class="edit-table" style='width:1409px;'>
        <tr>
            <th>序号</th>
            <th>状态</th>
            <th>姓名</th>
            <th>学号</th>
        </tr>
    @for (int i = 0; i < Model.Count(); i++)
    {
        var item = Model[i];
        <tr>
            <td>@(i+1)</td>
            <td>
                @("成功") 
            </td>
            <td>@(item.StudentName)</td>
            <td>@(item.StudentNumber)</td>
        </tr>
    }
    </table>
    <script type="text/javascript">
        $(function () {
            $("#divAchievementStudentImportExcelResult").removeAttr("height");
            var t_height = $("#divAchievementStudentImportExcelResult").height();
            if (t_height > 500) {
                $("#divAchievementStudentImportExcelResult").attr("height", "500px");
            }
        });
    </script>
}
else
{
    <p>无法找到任何可导入的信息</p>
}
