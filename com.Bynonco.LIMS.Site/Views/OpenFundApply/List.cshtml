@using com.Bynonco.JqueryEasyUI.Core;
@{
    var openFundApplyPrivilige = com.Bynonco.LIMS.BLL.Business.Privilige.PriviligeFactory.GetOpenFundApplyPrivilige(User.Identity.Name);
    var dataGrid = Html.JDataGrid("tbOpenFundApplyList")
       .SetUrl(ViewData["Url"].ToString())
       .SetHeight(650)
       .SetSortName("ApplyTime D,ApplyNum")
       .SetSortOrder(SortOrder.desc)
       .SetPageSize(10)
       .SetMutiSelect(false)
       .SetSingleSelect(true)
       .SetShowPrintButton(false)
       .SetShowSearchButton(true)
       .SetSearchScript("doShowSearch();");
    dataGrid.SetShowAddButton(false);
    dataGrid.SetShowEditButton(false);
    dataGrid.SetShowDeleteButton(false);
    if (openFundApplyPrivilige.IsEnableAdd)
    {
       dataGrid.SetAddScript("doAddOpenFundApply('');");
    }
    dataGrid.AddFrozenColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("编码", "Id").SetSortable(false).SetHidden(true));
    dataGrid.AddFrozenColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("申请人", "Icon").SetWidth(45).SetAlign(Align.center).SetSortable(false));
    dataGrid.AddFrozenColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("申请单号", "ApplyNumStr").SetWidth(250).SetAlign(Align.left));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("申请人", "UserName").SetSortable(false).SetWidth(70).SetAlign(Align.center));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("可用设备", "EquipmentNames").SetSortable(false).SetWidth(150).SetAlign(Align.left));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("设备管理员", "DirectorName").SetSortable(false).SetWidth(150).SetAlign(Align.left));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("设备单位", "EquipmentOrgNames").SetSortable(false).SetWidth(150).SetAlign(Align.left));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("可用补贴余额", "BalanceMoneyStr").SetWidth(80).SetAlign(Align.center));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("状态", "StatusStr").SetSortable(false).SetWidth(70).SetAlign(Align.center));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("存款自付金额", "DepositRealCurrencySumStr").SetSortable(false).SetWidth(80).SetAlign(Align.center));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("存款补贴基金", "DepositSubsidyCurrencySumStr").SetSortable(false).SetWidth(80).SetAlign(Align.center));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("存款总额", "DepositCurrencySumStr").SetSortable(false).SetWidth(80).SetAlign(Align.center));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("结算补贴金额", "ClosedMoneyStr").SetSortable(false).SetWidth(80).SetAlign(Align.center));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("申请单位", "UserOrgName").SetWidth(100).SetAlign(Align.center).SetSortable(false));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("申请时间", "ApplyTimeStr").SetWidth(80).SetAlign(Align.center));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("创建人", "CreateName").SetWidth(80).SetAlign(Align.center));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("创建时间", "CreateTimeStr").SetWidth(120).SetAlign(Align.center));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("修改人", "UpdateUserName").SetWidth(80).SetAlign(Align.center));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("修改时间", "UpdateTimeStr").SetWidth(120).SetAlign(Align.center));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("审核人", "AuditUserName").SetWidth(80).SetAlign(Align.center));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("审核时间", "AuditTimeStr").SetWidth(120).SetAlign(Align.center));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("审核备注", "AuditRemark").SetWidth(120).SetAlign(Align.center));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("结算人", "ClosedUserName").SetWidth(80).SetAlign(Align.center));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("结算时间", "ClosedTimeStr").SetWidth(120).SetAlign(Align.center));
    dataGrid.AddColum(new com.Bynonco.JqueryEasyUI.Core.DataGridColumn("备注", "Remark").SetWidth(300).SetAlign(Align.left));
    if (openFundApplyPrivilige.IsEnableImportExcel)
    {
        dataGrid.AddToolBarButton(new ToolBarButton() { Disabled = false, Handler = "doImportExcel();", IconCls = "icon-import", Id = "btnImportExcel", Text = "导入" });
    }
    if (openFundApplyPrivilige.IsEnableExportExcel)
    {
        dataGrid.AddToolBarButton(new ToolBarButton() { Disabled = false, Handler = "doExportExcel();", IconCls = "icon-excel", Id = "btnExportExcel", Text = "导出" });
    }
    
 }
@(dataGrid.ToMvcString())