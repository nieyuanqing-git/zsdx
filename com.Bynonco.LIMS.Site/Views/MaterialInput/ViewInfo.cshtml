@model com.Bynonco.LIMS.Model.View.ViewMaterialInput

<div id="divMaterialInputView">
@if (Model != null)
{
    <table  class="edit-table"  style="width:530px;height:auto">
        <tr>
            <th>入库单号</th>
            <td>@Model.InputNum</td>
            <th>采购单号</th>
            <td>@Model.PurchaseNum</td>
        </tr>
        <tr>
            <th>入库人</th>
            <td>@Model.InputUserName</td>
            <th>入库时间</th>
            <td>@Model.InputTimeStr</td>
        </tr>
        <tr>
            <th>详情</th>
            <td colspan='3'>@System.Web.Mvc.MvcHtmlString.Create(Model.ItemDetail)</td>
        </tr>
        <tr>
            <th>备注</th>
            <td colspan='3'>@System.Web.Mvc.MvcHtmlString.Create(Model.Remark)</td>
        </tr>
        <tr>
            <th>创建人</th>
            <td>@Model.CreaterName</td>
            <th>创建时间</th>
            <td>@Model.CreateTimeStr</td>
        </tr>
        <tr>
            <th>修改人</th>
            <td>@Model.UpdateUserName</td>
            <th>修改时间</th>
            <td>@Model.UpdateTimeStr</td>
        </tr>
    </table>
}
else
{
    @:无效信息
}
</div> 
